{
  "compilerOptions": {
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "diagnostics": true,
    "noEmit": false,
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "allowJs": true,
    "declaration": false,
    "experimentalDecorators": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitUseStrict": false,
    "removeComments": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "pretty": true,
    "lib": [
      "dom",
      "es2020"
    ],
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es2020",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "listFiles": false,
    "noEmitHelpers": true,
    "jsx": "react",
    "jsxFactory": "h",
    "outDir": "www"
  },
  "include": [
    "src",
    "src/**/*"
  ],
  "exclude": [
    "**/*.d.ts",
    "www",
    "node_modules"
  ],
  "compileOnSave": false
}